{
	"name": "poweredbycoffee/wpmail-mailgun",
	"description": "Drop-in to use MailGun for sending email from WordPress",
	"license": "gpl2",
	"type":"wordpress-muplugin",
	"authors": [
		{
			"name": "Stewart Ritchie",
			"email": "stewart@poweredbycoffee.co.uk"
		}
	],
	"require": {
		"mailgun/mailgun-php": "^4.0",
		"nyholm/psr7": "^1.8",
		"symfony/http-client": "^6.4"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		},
		"platform": {
			"php": "8.1"
		}		
	}
}
